<plugin name='google-map' version='0.1' grailsVersion='1.3.0 &gt; *'>
  <author>Bhagwat Kumar</author>
  <authorEmail>bhagwat@intelligrape.com</authorEmail>
  <title>Google Map</title>
  <description>\
Integrate Google map version 3 into your grails application
</description>
  <documentation>http://grails.org/plugin/google-map</documentation>
  <resources>
    <resource>BuildConfig</resource>
    <resource>DataSource</resource>
    <resource>DefaultGoogleMapConfig</resource>
    <resource>UrlMappings</resource>
    <resource>org.grails.plugin.googlemap.taglib.MapTagLib</resource>
  </resources>
  <dependencies>
    <plugin name='jquery' version='1.4.2 &gt; *' />
  </dependencies>
  <behavior />
</plugin>